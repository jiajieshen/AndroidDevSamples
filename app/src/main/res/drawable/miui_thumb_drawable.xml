<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="#65a9fd" />
            <stroke android:width="1px" android:color="#65a9fd" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="#cacaca" />
            <stroke android:width="1px" android:color="#cccccc" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="#65a9fd" />
            <stroke android:width="1px" android:color="#65a9fd" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#cacaca" />
            <stroke android:width="1px" android:color="#cccccc" />
        </shape>
    </item>
</selector>